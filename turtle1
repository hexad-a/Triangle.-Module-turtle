from turtle import *
pensize(3)
speed(10)


# 7

def rama() :
    penup()
    goto(-528, -10)
    pendown()
    color('black')
    begin_fill()
    for i in range(2) :
        forward(6)
        right(90)
        forward(80)
        right(90)
    end_fill()
    penup()
    goto(-550, -48)
    pendown()
    color('black')
    begin_fill()
    for i in range(2) :
        forward(50)
        right(90)
        forward(6)
        right(90)
    end_fill()



# 12

def dymohod() :
    penup()
    goto(-373, 180)
    pendown()
    color('black')
    begin_fill()
    left(175)
    forward(70)
    right(90)
    forward(40)
    right(90)
    forward(141)
    left(90)
    end_fill()    


