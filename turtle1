# 13

def dym() :
    penup()
    goto(-370, 275)
    pendown
    color('light gray')
    begin_fill()
    circle(17)
    end_fill()
    penup()
    goto(-350, 293)
    pendown()
    color('light gray')
    begin_fill()
    circle(27)
    end_fill()
    penup()
    goto(-370, 318)
    pendown()
    color('light gray')
    begin_fill()
    circle(35)
    right(2)
    end_fill()



# 10

def derevo() :
    penup()
    goto(180, -250)
    pendown()
    color('brown')
    begin_fill()
    left(85)
    forward(200)
    right(84)
    forward(30)
    right(85)
    forward(200)
    end_fill()
    penup()
    goto(130, -20)
    pendown
    color('light green')
    begin_fill()
    circle(85)
    end_fill()



# 3

def asphalt() :
    penup()
    goto(500, -150)
    pendown()
    color('black')
    begin_fill()
    forward(230)
    right(80)
    forward(400)
    right(100)
    forward(390)
    right(100)
    forward(401)
    right(80)
    end_fill()
    penup()
    goto(501, -150)
    pendown()
    color('white')
    begin_fill()
    forward(10)
    right(100)
    forward(300)
    right(80)
    forward(10)
    right(100)
    forward(300)
    right(80)
    end_fill()
    penup()
    goto(697, -150)
    pendown()
    forward(10)
    right(80)
    begin_fill()
    forward(300)
    right(100)
    forward(10)
    right(80)
    forward(300)
    right(100)
    end_fill()
    penup()
    goto(600, -150)
    pendown()
    begin_fill()
    forward(10)
    right(90)
    forward(60)
    right(90)
    forward(10)
    right(90)
    forward(60)
    right(90)
    end_fill()
    penup()
    goto(600, -237)
    pendown()
    begin_fill()
    forward(10)
    right(90)
    forward(60)
    right(90)
    forward(10)
    right(90)
    forward(60)
    right(90)
    end_fill()



